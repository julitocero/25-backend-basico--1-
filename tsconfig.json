{
  "compilerOptions": {
    "typeRoots": ["./src/types", "./node_modules/@types"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}

